{
  "name": "puppeteer-brillo",
  "version": "1.0.3",
  "description": "Puppeteer tools to help you scrape websites",
  "repository": {
    "type": "git",
    "url": "https://github.com/OrthoFi/puppeteer-brillo.git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk node_modules/.bin/jest"
  },
  "author": "Brian Schiller",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.spec\\.ts$"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "puppeteer": "*"
  },
  "dependencies": {
    "@types/puppeteer": "^3.0.2",
    "puppeteer": "^5.3.1"
  }
}
